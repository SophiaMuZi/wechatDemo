/**public **/
view{
  box-sizing: border-box;
}
view,text{
  font-size: 28rpx;
}
image{
  width: 100%;
  height: 100%;
  vertical-align: middle;
}


.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #f0f0f0;
} 
.flex{
  display: flex;
} 
.flex-jcsb{
  display: flex;
  justify-content: space-between;
}
.flex-wrp{
  display:flex;
  flex-wrap: wrap;
}
.flex-item{
  flex-grow: 1;
}
